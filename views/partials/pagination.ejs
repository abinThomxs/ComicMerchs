  <ul class="pagination justify-content-center">
    <% for ( let i=1; i <=pages; i++) { %>
      <% if ( i==pageNum ) {%>
        <li class="page-item active px-2"><a class="page-link" href="http://localhost:3000/user/store/?page=<%= i %>">
            <%= i %>
          </a></li>
        <% } else { %>
          <li class="page-item px-2"><a class="page-link" href="http://localhost:3000/user/store/?page=<%= i %>">
              <%= i %>
            </a></li>
          <% } %>
            <% } %>
